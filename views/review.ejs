<h1>  These are the checkins for <%= fname %> <%= lname %></h1>
<div class="childlist">
	 <% checkins.forEach(function(checkin){ %>
	      <h5>Checkin Time</h5>
	      <%= checkin.time%>
	      <h5>Checkin Latitude</h5>
	      <%= checkin.lat%>
	      <h5>Checkin Longitude</h5>
	      <%= checkin.long%>
	<% }) %>
</div>

<div id="floating-panel">
	<input id="latlng" type="text" value="<%= lat%>,<%= long%>">
	<input id="submit" type="button" value="Reverse Geocode">
</div>
<div id="map"></div>
	<script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBs6w_10kkDInZPZJBsmlSBxe_gQhyhepc&callback=initMap">
</script>
	<script src='/javascripts/geoloc.js' type="text/javascript">
</script>